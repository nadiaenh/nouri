{
  "manifest_version": 2,
  "name": "nouri",
  "description": "This extension tells you if the product you are shopping for is bad for your skin.",
  "version": "2.0",
  "background": {
    "service_worker": "./background.js"
  },
  "browser_action": {
      "default_icon": "assets/icon.png",
      "default_title": "nouri",
      "default_popup": "popup.html"
    }
}
